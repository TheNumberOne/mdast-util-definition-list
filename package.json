    "micromark-extension-definition-list": "github:wataru-chocola/micromark-extension-definition-list"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "prettier": "2.3.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {},
  "keywords": [

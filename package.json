{
  "name": "mdast-util-definition-list",
  "version": "1.0.0",
  "description": "mdast extension for definition list",
  "author": "Wataru Watanabe",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wataru-chocola/mdast-util-definition-list.git"
  },
  "bugs": {
    "url": "https://github.com/wataru-chocola/mdast-util-definition-list/issues"
  },
  "homepage": "https://github.com/wataru-chocola/mdast-util-definition-list",
  "main": "./index.js",
  "types": "./index.d.ts",
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "!lib/**/*.test.js",
    "!lib/**/*.test.d.ts",
    "index.d.ts",
    "index.js"
  ],
  "dependencies": {
    "@types/mdast": "^3.0.10",
    "mdast-util-from-markdown": "^1.0.0",
    "mdast-util-to-hast": "^11.2.0",
    "mdast-util-to-markdown": "^1.2.1",
    "micromark-extension-definition-list": "^1.0.0",
    "unist-builder": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-modules-commonjs": "^7.15.4",
    "@babel/preset-env": "^7.15.4",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "babel-jest": "^27.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "hast-util-to-html": "^8.0.2",
    "jest": "^27.1.0",
    "prettier": "2.3.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {},
  "keywords": [
    "mdast",
    "mdast-util",
    "markdown"
  ],
  "scripts": {
    "format": "prettier --write \"./**/*.{ts,json}\"",
    "lint": "eslint ./**/*.{ts}",
    "lint-fix": "eslint --fix ./**/*.{ts} && prettier --write ./**/*.{ts,json}",
    "test": "npm run build && jest 'test\\.js' --testPathIgnorePatterns=\"/node_modules/\"",
    "build": "npm run clean && tsc",
    "prepack": "npm run build",
    "clean": "rimraf index.js index.test.js lib/*.d.ts lib/*.js"
  }
}
